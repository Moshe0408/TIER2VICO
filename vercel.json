{
    "version": 2,
    "builds": [
        {
            "src": "Dashboard_App.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": "uploads/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Dashboard_App.py"
        }
    ]
}