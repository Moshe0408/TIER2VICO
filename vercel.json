{
    "version": 2,
    "builds": [
        {
            "src": ".",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Dashboard_App.py"
        }
    ]
}